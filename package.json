{
    "name": "@todoforai/puppeteer-mcp-server",
    "version": "0.7.12",
    "description": "Experimental MCP server for browser automation using Puppeteer (inspired by @modelcontextprotocol/server-puppeteer)",
    "license": "MIT",
    "author": "todoforai",
    "homepage": "https://github.com/todoforai/puppeteer-mcp-server",
    "bugs": "https://github.com/todoforai/puppeteer-mcp-server/issues",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/todoforai/puppeteer-mcp-server.git"
    },
    "type": "module",
    "bin": {
        "puppeteer-mcp-server": "dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && shx chmod +x dist/*.js",
        "prepare": "npm run build",
        "watch": "tsc --watch",
        "inspect": "npx @modelcontextprotocol/inspector node dist/index.js",
        "publish:patch": "npm version patch && npm publish",
        "publish:minor": "npm version minor && npm publish",
        "version": "npm view @todoforai/puppeteer-mcp-server version"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "1.0.1",
        "puppeteer": "^24.22.3",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.11.17",
        "shx": "^0.3.4",
        "typescript": "^5.6.2"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
